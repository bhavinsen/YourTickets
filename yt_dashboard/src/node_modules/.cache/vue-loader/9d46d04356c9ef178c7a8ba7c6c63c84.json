mplate>\n\n<script>\n  import LeftMainNavigation from './components/layout/left_main_navigation.vue'\n  import Appbar from './components/layout/appbar.vue'\n\n  export default {\n    components:{\n      LeftMainNavigation,\n      Appbar\n    },\n    mounted() {\n\n      this.$store.subscribe((mutation) => {\n        if(mutation.type === 'messages/add'){\n          this.snackbar_text = mutation.payload.text\n          this.snackbar = true\n          this.$store.dispatch('messages/remove', mutation.payload)\n        }\n      })\n\n    },\n    methods: {\n      togglenav() {\n        this.$store.commit('layout/navopen',!this.$store.state.layout.navopen)\n      }\n    },\n    watch:{\n      $route(to) {\n        // console.log('route check')\n        if(to.meta.isEventPage){\n          this.$store.getters['event/getEvent'](to.params.id)\n              .then((response) => {\n\n                this.$store.commit('layout/isEventPage', {\n                  isEventPage: true,\n                  event_id: to.params.id,\n                  event: response.data\n                })\n              })\n              .finally(() => this.loading = false)\n        }else{\n          this.$store.commit('layout/isEventPage', {'isEventPage':false, event_id:0})\n        }\n      }\n    },\n    data: () => ({\n      snackbar:false,\n      timeout:2000,\n      snackbar_text:''\n    }),\n  }\n</script>\n"]}]}